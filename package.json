{
  "name": "gatsby-example-using-ipynb",
  "private": true,
  "description": "Gatsby example site using the ipynb plugin",
  "version": "1.1.0",
  "author": "Benjamin Abel <dev.abel@laposte.net>",
  "dependencies": {
    "@nteract/gatsby-transformer-ipynb": "^1.0.1",
    "@nteract/notebook-preview": "^7.1.2",
    "gatsby": "latest",
    "gatsby-link": "latest",
    "gatsby-source-filesystem": "latest",
    "katex": "^0.10.0-beta",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "styled-jsx": "^2.2.6"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write 'src/**/*.js'",
    "prettier-watch": "onchange 'src/**/*.(js|md)' -- prettier --write {{changed}}",
    "watch-plugin": "rm -rf .cache;cd ..;npm run watch",
    "start": "rm -rf .cache && concurrently --kill-others \"npm run prettier-watch\" \"npm run develop\"",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "gh-pages": "^1.1.0",
    "onchange": "^3.3.0",
    "prettier": "^1.12.0"
  }
}
